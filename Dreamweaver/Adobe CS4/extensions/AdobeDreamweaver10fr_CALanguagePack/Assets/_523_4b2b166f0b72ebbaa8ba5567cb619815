					<td valign="top" colspan="2" class="CLEARSEPARATION">&#160;</td>
				</tr>
				<tr>
					<td><span class="programlisting"><pre>document.getElementById("myButton").dataSrc = "DBSRC3";</pre>
						</span></td>
				</tr>
