					<td valign="top" colspan="2" class="CLEARSEPARATION">&#160;</td>
				</tr>
				<tr>
					<td><span class="programlisting"><pre>var frameElem = parent.document.getElementById("myFrame");
var win = frameElem.contentWindow;</pre>
						</span></td>
				</tr>
