					<td valign="top" colspan="2" class="CLEARSEPARATION">&#160;</td>
				</tr>
				<tr>
					<td><span class="programlisting"><pre>document.myForm.myTextArea.dataSrc = "DBSRC3";</pre>
						</span></td>
				</tr>
