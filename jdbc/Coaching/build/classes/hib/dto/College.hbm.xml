<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="hibernate.beans">
    <class name="College" table="Colltab">
        <id name="registrationNo" column="regNo"/>
        <property name="collegeName" column="collegename"/>
        <property name="city" column="city"/>
        <property name="rank" column="rank"/>
        <property name="totalCourses" column="totalcourses"/>
        <property name="university" column="university"/>
    </class>
</hibernate-mapping>
<root>
    
</root>

